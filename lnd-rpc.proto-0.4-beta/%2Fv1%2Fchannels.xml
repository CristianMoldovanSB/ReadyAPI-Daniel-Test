<?xml version="1.0" encoding="UTF-8"?>
<con:resource id="a38061ed-be5e-4690-8c9f-484ad4fcee14" name="/v1/channels" path="/v1/channels" xmlns:con="http://eviware.com/soapui/config">
  <con:settings>
    <con:setting id="a38061ed-be5e-4690-8c9f-484ad4fcee14fileName">%2Fv1%2Fchannels</con:setting>
  </con:settings>
  <con:parameters/>
  <con:method id="85a3d737-e04e-4dbc-b52a-908fba4ebeaa" method="GET" name="ListChannels">
    <con:description>* lncli: `listchannels`
ListChannels returns a description of all the open channels that this node
is a participant in.</con:description>
    <con:settings/>
    <con:parameters/>
    <con:representation type="RESPONSE">
      <con:mediaType>application/json</con:mediaType>
      <con:status>200</con:status>
      <con:params/>
      <con:sampleContent>{
  "channels" : [ {
    "active" : true,
    "remote_pubkey" : "string",
    "channel_point" : "string",
    "chan_id" : "string",
    "capacity" : "string",
    "local_balance" : "string",
    "remote_balance" : "string",
    "commit_fee" : "string",
    "commit_weight" : "string",
    "fee_per_kw" : "string",
    "unsettled_balance" : "string",
    "total_satoshis_sent" : "string",
    "total_satoshis_received" : "string",
    "num_updates" : "string",
    "pending_htlcs" : [ {
      "incoming" : true,
      "amount" : "string",
      "hash_lock" : "string",
      "expiration_height" : 0
    } ],
    "csv_delay" : 0
  } ]
}</con:sampleContent>
    </con:representation>
    <con:request id="47a475a4-ae89-43da-924d-9250e189aa22" mediaType="application/json" name="Request 1">
      <con:settings/>
      <con:endpoint>https://api.swaggerhub.com</con:endpoint>
      <con:credentials>
        <con:selectedAuthProfile>No Authorization</con:selectedAuthProfile>
        <con:authType>No Authorization</con:authType>
      </con:credentials>
      <con:parameters/>
    </con:request>
  </con:method>
  <con:method id="17fd6a44-a160-41cf-82fd-9035241b8c94" method="POST" name="OpenChannelSync">
    <con:description>*
OpenChannelSync is a synchronous version of the OpenChannel RPC call. This
call is meant to be consumed by clients to the REST proxy. As with all
other sync calls, all byte slices are intended to be populated as hex
encoded strings.</con:description>
    <con:settings/>
    <con:parameters/>
    <con:representation type="REQUEST">
      <con:mediaType>application/json</con:mediaType>
      <con:params/>
      <con:sampleContent>{
  "node_pubkey" : "string",
  "node_pubkey_string" : "string",
  "local_funding_amount" : "string",
  "push_sat" : "string",
  "target_conf" : 0,
  "sat_per_byte" : "string",
  "private" : true,
  "min_htlc_msat" : "string"
}</con:sampleContent>
    </con:representation>
    <con:representation type="RESPONSE">
      <con:mediaType>application/json</con:mediaType>
      <con:status>200</con:status>
      <con:params/>
      <con:sampleContent>{
  "funding_txid_bytes" : "string",
  "funding_txid_str" : "string",
  "output_index" : 0
}</con:sampleContent>
    </con:representation>
    <con:request id="99b154d4-469e-4c1f-9486-30aa70ceb096" mediaType="application/json" name="Request 1">
      <con:settings/>
      <con:endpoint>https://api.swaggerhub.com</con:endpoint>
      <con:request>{\r
  "node_pubkey" : "string",\r
  "node_pubkey_string" : "string",\r
  "local_funding_amount" : "string",\r
  "push_sat" : "string",\r
  "target_conf" : 0,\r
  "sat_per_byte" : "string",\r
  "private" : true,\r
  "min_htlc_msat" : "string"\r
}</con:request>
      <con:credentials>
        <con:selectedAuthProfile>No Authorization</con:selectedAuthProfile>
        <con:authType>No Authorization</con:authType>
      </con:credentials>
      <con:parameters/>
    </con:request>
  </con:method>
</con:resource>
